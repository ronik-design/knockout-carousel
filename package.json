{
  "name": "knockout-carousel",
  "version": "0.9.0",
  "description": "An efficient, configurable carousel for knockout.js",
  "main": "dist/knockout-carousel.js",
  "jsnext:main": "lib/knockout-carousel.js",
  "scripts": {
    "build": "mkdir -p dist && npm run build:js && npm run build:css",
    "build:js": "rollup -c",
    "build:css": "node-sass lib/knockout-carousel.scss dist/knockout-carousel.css",
    "test": "eslint lib",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ronik-design/knockout-carousel.git"
  },
  "keywords": [
    "knockout",
    "binding",
    "init",
    "observables",
    "variables",
    "data"
  ],
  "author": {
    "name": "Michael Shick (Ronik Design)",
    "email": "contact@ronikdesign.com",
    "url": "http://www.ronikdesign.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ronik-design/knockout-carousel/issues"
  },
  "homepage": "https://github.com/ronik-design/knockout-carousel#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015-rollup": "^1.0.0",
    "eslint": "^1.10.3",
    "eslint-config-defaults": "^8.0.2",
    "eslint-plugin-filenames": "^0.2.0",
    "node-sass": "^3.4.2",
    "rollup": "^0.25.4",
    "rollup-plugin-babel": "^2.4.0"
  },
  "dependencies": {
    "jquery.event.move": "^1.3.6",
    "jquery.event.swipe": "^0.5.4"
  }
}
